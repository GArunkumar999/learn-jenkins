pipeline{
    agent any
    stages{
        stage('build'){
            steps{
                script{
                      echo "THIS IS BUILD"
              
                }
            }
        }
        stage('test'){
            steps{
                 script{
                      echo "THIS IS TEST"
              
                }
            }
        }
        stage('scan'){
            steps{
                script{
                    echo "THIS IS SCAN"
                }
            }
        }
        stage('deploy'){
            steps{
                 script{
                      echo "THIS IS DEPLOY"
              
                }
            }
        }
    }
}